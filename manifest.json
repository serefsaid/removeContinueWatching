{
    "manifest_version": 3,
    "name": "Remove Continue Watching",
    "description": "Remove contents from your countinue watching list on disney+",
    "version": "1.1",
    "icons": {
        "48": "images/removeContinueWatching.png"
    },
    "content_scripts": [
        {
        "js": [
            "remover.js"
        ],
        "matches": [
            "https://www.disneyplus.com/*"
        ]
        }
    ]
}